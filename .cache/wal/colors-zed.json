{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#0D080C90",
                "editor.background": "#0D080C90",
                "editor.foreground": "#c2c1c2",
                "text": "#c2c1c2",
                "text.muted": "#c2c1c270",
                "text.ignored": "#c2c1c240",
                "text.placeholder": "#c2c1c250",
                "ignored": "#c2c1c230",
                "element.hover": "#c2c1c230",
                "ghost_element.hover": "#4b3b4b30",
                "ghost_element.selected": "#4b3b4b30",
                "ghost_element.active": "#4b3b4b60",
                "border": "#0D080C",
                "editor.highlighted_line.background": "#4b3b4b10",
                "editor.active_line.background": "#4b3b4b10",
                "panel.background": "#0D080C90",
                "title_bar.background": "#0D080C90",
                "title_bar.inactive_background": "#0D080C90",
                "status_bar.background": "#0D080C90",
                "drop_target.background": "#0D080C90",
                "elevated_surface.background": "#0D080C",
                "toolbar.background": "#0D080C90",
                "tab_bar.background": "#0D080C90",
                "tab.inactive_background": "#0D080C90",
                "tab.active_background": "#4b3b4b30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#0D080C",
                "scrollbar.thumb.background": "#4b3b4b",
                "editor.gutter.background": "#0D080C90",
                "terminal.background": "#0D080C10",
                "terminal.foreground": "#c2c1c2",
                "terminal.dim_foreground": "#c2c1c2",
                "terminal.bright_foreground": "#c2c1c2",
                "terminal.ansi.black": "#0D080C",
                "terminal.ansi.red": "#4b3b4b",
                "terminal.ansi.green": "#6b2751",
                "terminal.ansi.yellow": "#763a51",
                "terminal.ansi.blue": "#5b3565",
                "terminal.ansi.magenta": "#813975",
                "terminal.ansi.cyan": "#843675",
                "terminal.ansi.white": "#968a92",
                "terminal.ansi.bright_black": "#695569",
                "terminal.ansi.bright_red": "#654F64",
                "terminal.ansi.bright_green": "#8F346D",
                "terminal.ansi.bright_yellow": "#9E4E6C",
                "terminal.ansi.bright_blue": "#7A4787",
                "terminal.ansi.bright_magenta": "#AC4C9D",
                "terminal.ansi.bright_cyan": "#B0489D",
                "terminal.ansi.bright_white": "#c2c1c2",
                "modified": "#4b3b4b",
                "syntax": {
                    "attribute": {
                        "color": "#4b3b4b"
                    },
                    "boolean": {
                      "color": "#6b2751"
                    },
                    "comment": {
                      "color": "#695569"
                    },
                    "comment.doc": {
                      "color": "#695569"
                    },
                    "constant": {
                      "color": "#8F346D"
                    },
                    "function": {
                      "color": "#B0489D"
                    },
                    "keyword": {
                      "color": "#5b3565"
                    },
                    "number": {
                      "color": "#813975"
                    },
                    "operator": {
                      "color": "#5b3565"
                    },
                    "string": {
                      "color": "#4b3b4b"
                    },
                    "variable": {
                      "color": "#6b2751"
                    }
                },
                "players": [
                  {
                    "cursor": "#4b3b4b",
                    "background": "#0D080C",
                    "selection": "#4b3b4b30"
                  }
                ]
            }
        }
    ]
}
