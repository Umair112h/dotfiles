{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#09111290",
                "editor.background": "#09111290",
                "editor.foreground": "#c1c3c3",
                "text": "#c1c3c3",
                "text.muted": "#c1c3c370",
                "text.ignored": "#c1c3c340",
                "text.placeholder": "#c1c3c350",
                "ignored": "#c1c3c330",
                "element.hover": "#c1c3c330",
                "ghost_element.hover": "#45785730",
                "ghost_element.selected": "#45785730",
                "ghost_element.active": "#45785760",
                "border": "#091112",
                "editor.highlighted_line.background": "#45785710",
                "editor.active_line.background": "#45785710",
                "panel.background": "#09111290",
                "title_bar.background": "#09111290",
                "title_bar.inactive_background": "#09111290",
                "status_bar.background": "#09111290",
                "drop_target.background": "#09111290",
                "elevated_surface.background": "#091112",
                "toolbar.background": "#09111290",
                "tab_bar.background": "#09111290",
                "tab.inactive_background": "#09111290",
                "tab.active_background": "#45785730",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#091112",
                "scrollbar.thumb.background": "#457857",
                "editor.gutter.background": "#09111290",
                "terminal.background": "#09111210",
                "terminal.foreground": "#c1c3c3",
                "terminal.dim_foreground": "#c1c3c3",
                "terminal.bright_foreground": "#c1c3c3",
                "terminal.ansi.black": "#091112",
                "terminal.ansi.red": "#457857",
                "terminal.ansi.green": "#54815b",
                "terminal.ansi.yellow": "#6d845a",
                "terminal.ansi.blue": "#598660",
                "terminal.ansi.magenta": "#678c62",
                "terminal.ansi.cyan": "#7d9866",
                "terminal.ansi.white": "#8c9497",
                "terminal.ansi.bright_black": "#576b6b",
                "terminal.ansi.bright_red": "#5CA074",
                "terminal.ansi.bright_green": "#71AC7A",
                "terminal.ansi.bright_yellow": "#92B178",
                "terminal.ansi.bright_blue": "#77B381",
                "terminal.ansi.bright_magenta": "#8ABB83",
                "terminal.ansi.bright_cyan": "#A7CB89",
                "terminal.ansi.bright_white": "#c1c3c3",
                "modified": "#457857",
                "syntax": {
                    "attribute": {
                        "color": "#457857"
                    },
                    "boolean": {
                      "color": "#54815b"
                    },
                    "comment": {
                      "color": "#576b6b"
                    },
                    "comment.doc": {
                      "color": "#576b6b"
                    },
                    "constant": {
                      "color": "#71AC7A"
                    },
                    "function": {
                      "color": "#A7CB89"
                    },
                    "keyword": {
                      "color": "#598660"
                    },
                    "number": {
                      "color": "#678c62"
                    },
                    "operator": {
                      "color": "#598660"
                    },
                    "string": {
                      "color": "#457857"
                    },
                    "variable": {
                      "color": "#54815b"
                    }
                },
                "players": [
                  {
                    "cursor": "#457857",
                    "background": "#091112",
                    "selection": "#45785730"
                  }
                ]
            }
        }
    ]
}
