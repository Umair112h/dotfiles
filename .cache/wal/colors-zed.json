{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#0A060B90",
                "editor.background": "#0A060B90",
                "editor.foreground": "#c1c0c2",
                "text": "#c1c0c2",
                "text.muted": "#c1c0c270",
                "text.ignored": "#c1c0c240",
                "text.placeholder": "#c1c0c250",
                "ignored": "#c1c0c230",
                "element.hover": "#c1c0c230",
                "ghost_element.hover": "#78493c30",
                "ghost_element.selected": "#78493c30",
                "ghost_element.active": "#78493c60",
                "border": "#0A060B",
                "editor.highlighted_line.background": "#78493c10",
                "editor.active_line.background": "#78493c10",
                "panel.background": "#0A060B90",
                "title_bar.background": "#0A060B90",
                "title_bar.inactive_background": "#0A060B90",
                "status_bar.background": "#0A060B90",
                "drop_target.background": "#0A060B90",
                "elevated_surface.background": "#0A060B",
                "toolbar.background": "#0A060B90",
                "tab_bar.background": "#0A060B90",
                "tab.inactive_background": "#0A060B90",
                "tab.active_background": "#78493c30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#0A060B",
                "scrollbar.thumb.background": "#78493c",
                "editor.gutter.background": "#0A060B90",
                "terminal.background": "#0A060B10",
                "terminal.foreground": "#c1c0c2",
                "terminal.dim_foreground": "#c1c0c2",
                "terminal.bright_foreground": "#c1c0c2",
                "terminal.ansi.black": "#0A060B",
                "terminal.ansi.red": "#78493c",
                "terminal.ansi.green": "#99533f",
                "terminal.ansi.yellow": "#786b54",
                "terminal.ansi.blue": "#a66c54",
                "terminal.ansi.magenta": "#437064",
                "terminal.ansi.cyan": "#a7816f",
                "terminal.ansi.white": "#918995",
                "terminal.ansi.bright_black": "#615567",
                "terminal.ansi.bright_red": "#A16250",
                "terminal.ansi.bright_green": "#CC6F54",
                "terminal.ansi.bright_yellow": "#A18F70",
                "terminal.ansi.bright_blue": "#DE9070",
                "terminal.ansi.bright_magenta": "#5A9686",
                "terminal.ansi.bright_cyan": "#DFAD94",
                "terminal.ansi.bright_white": "#c1c0c2",
                "modified": "#78493c",
                "syntax": {
                    "attribute": {
                        "color": "#78493c"
                    },
                    "boolean": {
                      "color": "#99533f"
                    },
                    "comment": {
                      "color": "#615567"
                    },
                    "comment.doc": {
                      "color": "#615567"
                    },
                    "constant": {
                      "color": "#CC6F54"
                    },
                    "function": {
                      "color": "#DFAD94"
                    },
                    "keyword": {
                      "color": "#a66c54"
                    },
                    "number": {
                      "color": "#437064"
                    },
                    "operator": {
                      "color": "#a66c54"
                    },
                    "string": {
                      "color": "#78493c"
                    },
                    "variable": {
                      "color": "#99533f"
                    }
                },
                "players": [
                  {
                    "cursor": "#78493c",
                    "background": "#0A060B",
                    "selection": "#78493c30"
                  }
                ]
            }
        }
    ]
}
